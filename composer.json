{
    "name": "ndinhbang/query-cache",
    "description": "A laravel package to cache database query result",
    "keywords": [
        "laravel",
        "cache",
        "query-cache",
        "eloquent-cache"
    ],
    "homepage": "https://github.com/ndinhbang/query-cache",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nguyen Dinh Bang",
            "email": "ndinhbang111@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ndinhbang\\QueryCache\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ndinhbang\\QueryCache\\QueryCacheServiceProvider"
            ],
            "aliases": {
                "QueryCache": "Ndinhbang\\QueryCache\\Facades\\QueryCache"
            }
        }
    }
}
